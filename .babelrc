{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": "last 2 versions, ie 10-11"
        },
        "modules": false
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
  ]
}